{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "console_title_template": "{{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "diamond",
          "foreground": "#011627",
          "background": "#21c7a8",
          "leading_diamond": "\u256d\u2500\ue0b6",
          "trailing_diamond": "\ue0b0",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}}  ",
          "properties": {
            "windows": "\uf871"
          }
        },
        {
          "type": "root",
          "style": "diamond",
          "foreground": "#ffeb95",
          "background": "#ef5350",
          "leading_diamond": "<transparent,#ef5350>\ue0b0</>",
          "trailing_diamond": "\ue0b0",
          "template": " \uf0e7 ",
          "properties": {
            "root_icon": "\uf292 "
          }
        },
        {
          "type": "path",
          "style": "diamond",
          "foreground": "#011627",
          "background": "#82AAFF",
          "leading_diamond": "<transparent,#82AAFF>\ue0b0</>",
          "trailing_diamond": "\ue0b0",
          "template": " {{ .Path }} ",
          "properties": {
            "folder_icon": "\uf07c ",
            "folder_separator_icon": " <#011627>\ue0b1</> ",
            "home_icon": "\uf7db ",
            "style": "agnoster"
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#011627",
          "background": "#22da6e",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#addb67{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f78c6c{{ end }}",
            "{{ if gt .Ahead 0 }}#21c7a8{{ end }}",
            "{{ if gt .Behind 0 }}#c792ea{{ end }}"
          ],
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} ",
          "properties": {
            "branch_icon": "\ue725 ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          }
        },
        {
          "type": "executiontime",
          "style": "diamond",
          "foreground": "#d6deeb",
          "background": "#575656",
          "leading_diamond": "<transparent,#575656>\ue0b0</>",
          "trailing_diamond": "\ue0b4",
          "template": " {{ .FormattedMs }} ",
          "properties": {
            "style": "roundrock",
            "threshold": 0
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "shell",
          "style": "diamond",
          "foreground": "#011627",
          "background": "#d6deeb",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "<transparent,#d6deeb>\ue0b2</>",
          "template": "\uf489 {{ .Name }}"
        },
        {
          "type": "python",
          "style": "diamond",
          "foreground": "#FFE873",
          "background": "#306998",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#306998>\ue0b2</>",
          "template": "\ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}"
        },
        {
          "type": "julia",
          "style": "diamond",
          "foreground": "#359a25",
          "background": "#945bb3",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#945bb3>\ue0b2</>",
          "template": "<#ca3c34>\ue624</> {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}"
        },
        {
          "type": "aws",
          "style": "diamond",
          "foreground": "#faa029",
          "background": "#565656",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#565656>\ue0b2</>",
          "template": "\ue7ad {{.Profile}}{{if .Region}}@{{.Region}}{{end}}"
        },
        {
          "type": "kubectl",
          "style": "diamond",
          "foreground": "#ffffff",
          "background": "#316ce4",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#316ce4>\ue0b2</>",
          "template": "\ufd31 {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}}"
        },
        {
          "type": "time",
          "style": "diamond",
          "foreground": "#d6deeb",
          "background": "#234d70",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0b4",
          "template": "\uf64f {{ .CurrentDate | date .Format }}",
          "properties": {
            "time_format": "15:04:05"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#21c7a8",
          "template": "\u2570\u2500"
        },
        {
          "type": "exit",
          "style": "plain",
          "foreground": "#22da6e",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ef5350{{ end }}"
          ],
          "template": "\ue285\ue285 ",
          "properties": {
            "always_enabled": true
          }
        }
      ],
      "newline": true
    }
  ],
  "transient_prompt": {
    "foreground": "#d6deeb",
    "background": "transparent",
    "template": "\ue285 "
  },
  "osc99": true
}
